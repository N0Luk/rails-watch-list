<h1>All Lists</h1>
<div class="actions">
  <%= link_to "New List", new_list_path, class: "btn btn-primary" %>
</div>

<% @lists.each do |list| %>
  <div class="list">
    <h2><%= link_to list.name, list_path(list) %></h2>
    <p><%= pluralize(list.bookmarks.count, "bookmark") %></p>
  </div>
<% end %>
